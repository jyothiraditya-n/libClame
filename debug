cc -std=c99 -Wall -Wextra -Werror -Wpedantic -g -Og -Iinc/ -c src/error.c -o build/src/error.o
cc -std=c99 -Wall -Wextra -Werror -Wpedantic -g -Og -Iinc/ -c src/main.c -o build/src/main.o
c++ -std=c++20 -Wall -Wextra -Werror -Wpedantic -g -Og -Iinc/ -c src/generics_basic.cpp -o build/src/generics_basic_cpp.o
c++ -std=c++20 -Wall -Wextra -Werror -Wpedantic -g -Og -Iinc/ -c src/generics_ints.cpp -o build/src/generics_ints_cpp.o
c++ -std=c++20 -Wall -Wextra -Werror -Wpedantic -g -Og -Iinc/ -c src/generics_uints.cpp -o build/src/generics_uints_cpp.o
c++ -std=c++20 -Wall -Wextra -Werror -Wpedantic -g -Og -Iinc/ -c src/main.cpp -o build/src/main_cpp.o
ar -r build/libClame.a build/src/error.o build/src/main.o build/src/generics_basic_cpp.o build/src/generics_ints_cpp.o build/src/generics_uints_cpp.o build/src/main_cpp.o
